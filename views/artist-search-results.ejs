<link rel="stylesheet" href="/styles/artist-result.css">
<div class="container">
    <!-- <h1 style="margin: 0;">Artist Search Result</h1> -->

    <% allArtists.forEach((artist) => { %>
    <div class="artist">
        <!-- <a href="<%#- artist.items[i].images[0].url%>">Pic</a> -->
        <div style="background-image: url('<%= artist.images[0].url%>'); height: 400px; width: 350px; background-size: cover;"></div>
        <div class="texts">
            <h1><%- artist.name%></h1>
            <!-- <img src="<%#= artist.images[0].url%>" alt="picture"> -->
            <button><a href="/albums/<%- artist.id%>">View Albums</a></button>
        </div>
    </div>
    <% }) %>
</div>